<?xml version="1.0" encoding="utf-8"?>

<PropertyList>

  <path>poignee.ac</path>
  <animation>
    <!-- object name in ac file -->
    <object-name>poignee.poignee</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>poignee.poignee</object-name>
    <condition>
      <not>
        <property>sim/rendering/rembrandt/enabled</property>
      </not>
    </condition>
    <emission>
      <red>   0.13 </red>
      <green> 0.13 </green>
      <blue>  0.13 </blue>
    </emission>
  </animation>

</PropertyList>
