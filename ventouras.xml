<?xml version="1.0"?>
<PropertyList>
<comment>
<![CDATA[
Place this file in <FlightGearDirectory>/data/Protocol

Usage:
fgfs --aircraft=m2000-5 --timeofday=morning --generic=socket,in,25,,5500,udp,ventouras --generic=socket,out,25,localhost,5501,udp,ventouras  --generic=socket,out,25,YOUR_OCULUS_IP_HERE,5501,udp,ventouras
Replace YOUR_OCULUS_IP_HERE with IP address of Oculus (displayed when project app starts)
Outbound connection to localhost is only needed for testing locally (i.e. in Unity editor)
Outbound connection to Oculus is not needed if you use the connection_broker.py script
]]>
</comment>
<generic>

  <output>
    <line_separator>newline</line_separator>
    <var_separator>tab</var_separator>
    <binary_mode>false</binary_mode>

   <chunk>
    <name>latitude (deg)</name>
    <type>double</type>
    <format>%.6f</format>
    <node>/position/latitude-deg</node>
   </chunk>

   <chunk>
    <name>longitude (deg)</name>
    <type>double</type>
    <format>%.6f</format>
    <node>/position/longitude-deg</node>
   </chunk>

   <chunk>
    <name>altitude (m)</name>
    <type>double</type>
    <format>%.3f</format>
    <node>/position/altitude-ft</node>
    <factor>0.3048</factor>                  <!-- feet to meter -->
   </chunk>

   <chunk>
    <name>Velocity North ("vn" mps)</name>
    <type>float</type>
    <format>%.3f</format>
    <node>/velocities/speed-north-fps</node>
    <factor>0.3048</factor>                  <!-- fps to mps -->
   </chunk>

   <chunk>
    <name>Velocity East ("ve" mps)</name>
    <type>float</type>
    <format>%.3f</format>
    <node>/velocities/speed-east-fps</node>
    <factor>0.3048</factor>                  <!-- fps to mps -->
   </chunk>

   <chunk>
    <name>Velocity Down ("vd" mps)</name>
    <type>float</type>
    <format>%.3f</format>
    <node>/velocities/speed-down-fps</node>
    <factor>0.3048</factor>                  <!-- fps to mps -->
   </chunk>

   <chunk>
    <name>yaw angle (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/orientation/heading-deg</node>
   </chunk>

   <chunk>
    <name>pitch angle (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/orientation/pitch-deg</node>
   </chunk>

   <chunk>
    <name>roll angle (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/orientation/roll-deg</node>
   </chunk>

   <chunk>
    <name>yaw rate (degps)</name>
    <type>float</type>
    <format>%.3f</format>
    <node>/orientation/roll-rate-degps</node>
   </chunk>

   <chunk>
    <name>pitch rate (degps)</name>
    <type>float</type>
    <format>%.3f</format>
    <node>/orientation/pitch-rate-degps</node>
   </chunk>

   <chunk>
    <name>roll rate (degps)</name>
    <type>float</type>
    <format>%.3f</format>
    <node>/orientation/roll-rate-degps</node>
   </chunk>

   <chunk>
    <name>time</name>
    <type>double</type>
    <format>%.3f</format>
    <node>/sim/time/steady-clock-sec</node>
   </chunk>

   <chunk>
    <name>magnetic heading (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/orientation/heading-magnetic-deg</node>
   </chunk>

   <chunk>
    <name>AP heading bug (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/autopilot/settings/heading-bug-deg</node>
   </chunk>

   <chunk>
    <name>display heading (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/instrumentation/efis/mfd/true-north</node>
   </chunk>

   <chunk>
    <name>alpha (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/orientation/alpha-deg</node>
   </chunk>

   <chunk>
    <name>beta (deg)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/orientation/side-slip-deg</node>
   </chunk>

   <chunk>
    <name>g load (g)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/accelerations/pilot-g</node>
   </chunk>

   <chunk>
    <name>airspeed (kts)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/velocities/airspeed-kt</node>
   </chunk>

   <chunk>
    <name>mach</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/velocities/mach</node>
   </chunk>

   <chunk>
    <name>ground speed (kts)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/velocities/groundspeed-kt</node>
   </chunk>

   <chunk>
    <name>vertical speed (fps)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/velocities/vertical-speed-fps</node>
   </chunk>

   <chunk>
    <name>indicated altitude (ft)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/instrumentation/altimeter/indicated-altitude-ft</node>
   </chunk>
 
   <chunk>
    <name>rad altitude (ft)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/position/altitude-agl-ft</node>
   </chunk>

   <chunk>
    <name>gear pos (0 closed 1 open)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/gear/gear[1]/position-norm</node>
   </chunk>

    <chunk>
      <name>weight on wheels</name>
      <type>bool</type>
      <node>/gear/gear[1]/wow</node>
    </chunk>

   <chunk>
    <name>AP target speed (kts)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/autopilot/settings/target-speed-kt</node>
   </chunk>

   <chunk>
    <name>acceleration (fps2)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/fdm/jsbsim/accelerations/udot-ft_sec2</node>
   </chunk>

   <chunk>
    <name>afterburner (0 off 1 full)</name>
    <type>float</type>
    <format>%.2f</format>
    <node>/engines/engine[0]/afterburner</node>
   </chunk>

    <chunk>
      <name>left gear</name>
      <type>int</type>
      <node>/instrumentation/gearBox/leftgear</node>
    </chunk>

    <chunk>
      <name>nose gear</name>
      <type>int</type>
      <node>/instrumentation/gearBox/nozegear</node>
    </chunk>

    <chunk>
      <name>right gear</name>
      <type>int</type>
      <node>/instrumentation/gearBox/rightgear</node>
    </chunk>

    <chunk>
      <name>gear red</name>
      <type>int</type>
      <node>/instrumentation/gearBox/gearRed</node>
    </chunk>

   <chunk>
	<name>n1 (%)</name>
	<type>float</type>	
	<format>%.1f</format>
	<node>/engines/engine/n1</node>
   </chunk>

   <chunk>
	<name>internal remaining fuel (kg)</name>
	<type>float</type>	
	<format>%.1f</format>
	<node>/systems/fuel/internal-remaining-fuel</node>
   </chunk>

   <chunk>
	<name>total fuel (kg)</name>
	<type>float</type>	
	<format>%.1f</format>
	<node>/consumables/fuel/total-fuel-kg</node>
   </chunk>

   <chunk>
	<name>volts DC</name>
	<type>float</type>	
	<format>%.1f</format>
	<node>/systems/electrical/volts</node>
   </chunk>

   <chunk>
	<name>bingo fuel (kg)</name>
	<type>float</type>	
	<format>%.1f</format>
	<node>/instrumentation/consumables/bingo_fuel</node>
   </chunk>

   <chunk>
	<name>local time (string)</name>
	<type>string</type>	
	<node>/sim/time/local-time-string</node>
   </chunk>

  </output>

  <input>
    <line_separator>newline</line_separator>
    <var_separator>tab</var_separator>

    <chunk>
      <name>aileron</name>
      <type>float</type>
      <format>%.3f</format>
      <node>/controls/flight/aileron</node>
    </chunk>

    <chunk>
      <name>elevator</name>
      <type>float</type>
      <format>%.3f</format>
      <node>/controls/flight/elevator</node>
    </chunk>

    <chunk>
      <name>throttle</name>
      <type>float</type>
      <format>%.3f</format>
      <node>/controls/engines/engine[0]/throttle</node>
    </chunk>

    <chunk>
      <name>rudder</name>
      <type>float</type>
      <format>%.3f</format>
      <node>/controls/flight/rudder</node>
    </chunk>

    <chunk>
      <name>gear-down</name>
      <type>bool</type>
      <node>/controls/gear/gear-down</node>
    </chunk>

    <chunk>
      <name>brake-parking</name>
      <type>float</type>
      <format>%.2f</format>
      <node>/controls/gear/brake-parking</node>
    </chunk>

    <chunk>
      <name>brake-left</name>
      <type>float</type>
      <format>%.2f</format>
      <node>/controls/gear/brake-left</node>
    </chunk>

    <chunk>
      <name>brake-right</name>
      <type>float</type>
      <format>%.2f</format>
      <node>/controls/gear/brake-right</node>
    </chunk>

    <chunk>
      <name>canopy</name>
      <type>float</type>
      <format>%.2f</format>
      <node>/sim/model/door-positions/crew/position-norm</node>
    </chunk>

  </input>

</generic>

</PropertyList>
